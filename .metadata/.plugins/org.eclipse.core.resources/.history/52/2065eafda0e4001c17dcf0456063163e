
public class practice02 {
	public static void main(String[] args) {
		
		int a[] = {1, 3, 4, 5, 6, 7, 6, 6, 1, 3};
		
		for(int i = 0; i < a.length; i++){
			System.out.print(a[i]+",");
		}
		
		for(int i = 0, n = a.length; i < n; i++){
			System.out.print( a[i] + ( i!=n-1 ? "," : "" ) );
		}
	}

}
